{
    "version": 2,
    "functions": {
        "apiRoutes": {
            "handler": "src/api.js",
            "memory": 1024
        },
        "pageRoutes": {
            "handler": "src/pages.js",
            "memory": 1024
        }
    },
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": ".vercel/functions/apiRoutes"
        },
        {
            "src": "/(.*)",
            "dest": ".vercel/functions/pageRoutes"
        }
    ]
}