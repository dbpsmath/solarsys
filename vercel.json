{
    "version": 2,
    "functions": {
        "apiRoutes": "src/api.js",
        "pageRoutes": "src/pages.js"
    },
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": ".vercel/functions/apiRoutes"
        },
        {
            "src": "/(.*)",
            "dest": ".vercel/functions/pageRoutes"
        }
    ]
}